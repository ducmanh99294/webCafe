{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "panels": [
    {
      "type": "graph",
      "title": "CPU Usage",
      "targets": [
        {
          "expr": "rate(process_cpu_seconds_total[1m])",
          "legendFormat": "{{instance}}"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Heap Memory Usage",
      "targets": [
        {
          "expr": "jvm_memory_used_bytes{area='heap'} / 1024 / 1024",
          "legendFormat": "Heap Memory (MB)"
        },
        {
          "expr": "jvm_memory_max_bytes{area='heap'} / 1024 / 1024",
          "legendFormat": "Max Heap (MB)"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Non-Heap Memory Usage",
      "targets": [
        {
          "expr": "jvm_memory_used_bytes{area='nonheap'} / 1024 / 1024",
          "legendFormat": "Non-Heap (MB)"
        },
        {
          "expr": "jvm_memory_max_bytes{area='nonheap'} / 1024 / 1024",
          "legendFormat": "Max Non-Heap (MB)"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Active Threads",
      "targets": [
        {
          "expr": "jvm_threads_live_threads",
          "legendFormat": "Live Threads"
        }
      ]
    },
    {
      "type": "graph",
      "title": "HTTP Requests Count",
      "targets": [
        {
          "expr": "rate(http_server_requests_seconds_count[1m])",
          "legendFormat": "{{uri}}"
        }
      ]
    }
  ],
  "schemaVersion": 36,
  "title": "Spring Boot Monitoring",
  "version": 1
}
